{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "nrl-draggable-dialog": [
        "dist/nrl-draggable-dialog"
      ],
      "nrl-draggable-dialog/*": [
        "dist/nrl-draggable-dialog/*"
      ],
      "nrl-resizable-box": [
        "dist/nrl-resizable-box"
      ],
      "nrl-resizable-box/*": [
        "dist/nrl-resizable-box/*"
      ],
      "nrl-view-manager": [
        "dist/nrl-view-manager"
      ],
      "nrl-view-manager/*": [
        "dist/nrl-view-manager/*"
      ],
      "nrl-ui-components": [
        "dist/nrl-ui-components"
      ],
      "nrl-ui-components/*": [
        "dist/nrl-ui-components/*"
      ]
    }
  }
}